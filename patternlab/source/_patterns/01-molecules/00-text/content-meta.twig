{% set props = {
    "byline":fake('name'),
    "date":"now"|date(content_date_format|default("M d, Y - g:i a")),
}%}

{% embed "@limestone/content-meta.twig" with props only %}
    {% block meta_taxonomy %}
                {% set taxo = fake('words',{nb: 2 + random(3)}) %}
            {% for w in taxo %}
                {{ w|capitalize }}
            {% if not loop.last %}, {% endif %}
            {% endfor %}
        {% endblock %}
{% endembed %}


