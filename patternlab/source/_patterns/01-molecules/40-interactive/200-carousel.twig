{% set slides %}
    {% for i in 0 .. 6 %}
        {% include 'components-media-with-caption' with {
            lazy: true, 
            level: level|default(2), 
            aspect : '16:9', 
            width: 300 
        } %}
    {% endfor %}
{% endset %}

{% set props = {
    'carousel_style':'',
    'carousel_variant':carousel_variant,
    'slides': slides
}%}

{% include "@limestone/carousel.twig" with props only %}