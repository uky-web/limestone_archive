{% set carousel_slides %}
    {% for i in 0 .. 6 %}
        {% include '@limestone/media-with-caption.twig' with {
            "media":media|default('image'),
            "media_component": media_type|default('image'),
            "media_component_src":fake('imageUrl',{width: 300, height: 168}), 
            "lazy": false, 
            "linked": true,
            "level": level|default(2), 
            "citation_name": fake('name'),
            "caption":text.editorial_paragraphs(1, 5)
        } %}
    {% endfor %}
{% endset  %}

{% set props = {
    "slab_variant" : slab_variant|default('slab--wildcat-white'),
    "headline_level":level|default(1),
	"headline_group_id":headline_group_id|default("headline-group"|uniqid),
	"headline_group_variant":headline_group_variant,
	"headline_super": super|default(fake('bs'))|titleize,
	"headline_head": head|default(fake('bs'))|titleize,
	"headline_sub": sub|default(fake('bs'))|titleize,
    "carousel_level": 3, 
    "carousel_variant" : carousel_variant, 
    "carousel_layout": carousel_layout,
    "carousel_slides": carousel_slides
}%}

{% include "@limestone/carousel-slab.twig" with props only %}
