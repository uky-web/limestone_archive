{% extends 'base.twig' %}


{% set formatted_text_props = {
	content: text.editorial_paragraphs(5, 10)
}%}

{% block main_content %}

{% embed '@limestone/slab.twig' with { slab_variant : 'slab--wildcat-white' } %}
    {% block slab_content %}
        {% for i in 2..6 %}
            {% include '@limestone/heading.twig' with { level: i, headline: 'H' ~ i ~ '. ' ~ fake('bs')|titleize } %}
	        {% include "@limestone/formatted-text.twig" with formatted_text_props only %}
        {% endfor %}
        {% for i in 2..6 %}
            {% include 'molecules-headline-group' with { level: i, super: 'H' ~ i ~ ' headline group', head: fake('bs')|titleize, sub: fake('catchPhrase') } %}
	        {% include "@limestone/formatted-text.twig" with formatted_text_props only %}
        {% endfor %}        
    {% endblock %}
{% endembed %}

{% endblock %}